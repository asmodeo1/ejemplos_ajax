	"Es entero": {
		"prefix": "isEntero",
		"body": [
			"if(isNaN(Number($1)) || isNaN(parseInt($2))) {",
			"\t$0",
			"}"
		],
		"description": "Comprueba si es un número entero"
	},
	"Es decimal": {
		"prefix": "isFloat",
		"body": [
			"if(isNaN(Number($1)) || isNaN(parseFloat($2))) {",
			"\t$0",
			"}"
		],
		"description": "Comprueba si es un número con decimales"
	},
	"getElementById": {
		"prefix": "di",
		"body": [
			"document.getElementById(\"$1\")"
		],
		"description": "Genera un document.getElementById"
	},
	"getElementsByClassName": {
		"prefix": "dc",
		"body": [
			"document.getElementsByClassName(\"$1\")"
		],
		"description": "Genera un document.getElementsByClassName"
	},
	"getElementsByTagName": {
		"prefix": "dt",
		"body": [
			"document.getElementsByTagName(\"$1\")"
		],
		"description": "Genera un document.getElementsByTagName"
	}
	,
	"querySelector": {
		"prefix": "dq",
		"body": [
			"document.querySelector(\"$1\")"
		],
		"description": "Genera un document.querySelector"
	}
	,
	"querySelectorAll": {
		"prefix": "dqa",
		"body": [
			"document.querySelectorAll(\"$1\")"
		],
		"description": "Genera un document.querySelectorAll"
	}
	,
	"addEventListener": {
